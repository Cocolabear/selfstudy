use std::io::{read_to_string, stdin};

fn main() {
    let mut o_cnt = 0;
    let mut res = false;

    for c in read_to_string(stdin()).unwrap().chars().skip(2) {
        match c {
            'o' => o_cnt += 1,
            _ => o_cnt = 0,
        }

        if o_cnt >= 3 {
            res = true;
            break;
        }
    }

    println!("{}", if res { "Yes" } else { "No" });
}
